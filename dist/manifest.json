{
    "manifest_version": 2,
    "name": "DevAssist",
    "version": "1.0",
    "description": "Open Side Panel",
    "icons": {
        "48": "icons/faltu-48.png"
    },
    "permissions": [],
    "browser_action": {
        "default_icon": {
            "16": "icons/faltu-48.png"
        }
    },
    "sidebar_action": {
        "default_icon": "icons/faltu-48.png",
        "default_title": "DevAssist",
        "default_panel": "index.html"
    },
    "content_scripts": [{
        "matches": ["*://*.google.com/*"]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}